{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/queue.go",
      "use": "@vercel/go",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "src": "api/checkin/index.go",
      "use": "@vercel/go",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "frontend/_next/static/$1"
    },
    {
      "src": "/api/queue",
      "dest": "api/queue.go"
    },
    {
      "src": "/api/eligible",
      "dest": "api/eligible/index.go"
    },
    { 
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}