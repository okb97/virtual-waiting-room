{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/queue.go",
      "use": "@vercel/go",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/queue",
      "dest": "api/queue.go"
    },
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ]
}