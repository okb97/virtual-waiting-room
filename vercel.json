{
  "rewrites": [
    {
      "source": "/api/queue",
      "destination": "/frontend/api/queue.go"
    }
  ],
  "builds": [
    { "src": "api/**/*.go", "use": "@vercel/go" },
    { "src": "frontend/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
}